                                  radius=d, fill=False, color='w', lw=1, ls='solid')
    ax.add_artist(neighborhood)
    
    plt.xlim(xmin=0, xmax=im.size[0])
    plt.ylim(ymin=0, ymax=im.size[1])
    plt.title(roi)
    plt.imshow(im)
    
    plt.savefig('{0}/friend_2d(r={1})(d={2}).png'.format(folder, r, d))
    plt.close(fig)

